{
  "name": "handsworkShift",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "pods": "cd ios && pod install",
    "android:release": "yarn android --mode=release",
    "ios:release": "yarn ios --mode Release",
    "permissions": "react-native setup-ios-permissions && yarn pods",
    "android:local": "yarn android --mode=local",
    "ios:local": "yarn ios --mode=local",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "gradle:clean": "cd android && ./gradlew clean",
    "postinstall": "npx patch-package && yarn pods",
    "reactotron:adb-reverse": "adb reverse tcp:9090 tcp:9090"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.82.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "react": "19.1.1",
    "react-native": "0.82.0",
    "react-native-safe-area-context": "^5.5.2",
    "reactotron-core-client": "^2.9.9",
    "reactotron-react-native": "^5.1.18"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/eslint-config": "0.82.0",
    "@react-native/metro-config": "0.82.0",
    "@react-native/typescript-config": "0.82.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
